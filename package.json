{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:parity": "vitest run src/audit/math_parity_golden.test.ts",
    "generate:golden-vectors": "python3 src/audit/python/generate_golden_vectors.py",
    "generate:protocol-artifacts": "node scripts/generate_protocol_artifacts.mjs",
    "test:paper-rigor": "vitest run src/audit/paper_stress_pack.test.ts src/audit/depin_decision_tree_fixtures.test.ts",
    "generate:paper-stress-report": "node --experimental-strip-types scripts/generate_paper_stress_report.ts",
    "skill:optimizer:quick": "node --experimental-strip-types skills/depin-simulation-optimizer/scripts/run_optimizer_diagnostics.ts --mode quick",
    "skill:optimizer:full": "node --experimental-strip-types skills/depin-simulation-optimizer/scripts/run_optimizer_diagnostics.ts --mode full",
    "skill:optimizer:scan:depin-solana": "node --experimental-strip-types scripts/run_depin_solana_cohort_scan.ts --mode quick",
    "ci:optimizer:gate": "node --experimental-strip-types scripts/check_optimizer_report.ts",
    "ci:optimizer:scan:depin-solana": "node --experimental-strip-types scripts/run_depin_solana_cohort_scan.ts --mode quick",
    "ci:optimizer": "npm run skill:optimizer:quick -- --profile ono_v3_calibrated && npm run ci:optimizer:gate -- --profile ono_v3_calibrated --mode quick",
    "ci:optimizer:portfolio": "npm run ci:optimizer && npm run ci:optimizer:scan:depin-solana",
    "mindsdb:export:quick": "node --experimental-strip-types scripts/export_mindsdb_dataset.ts --mode quick",
    "mindsdb:export:full": "node --experimental-strip-types scripts/export_mindsdb_dataset.ts --mode full",
    "mindsdb:query-pack:quick": "node --experimental-strip-types scripts/generate_mindsdb_query_pack.ts --mode quick",
    "mindsdb:query-pack:full": "node --experimental-strip-types scripts/generate_mindsdb_query_pack.ts --mode full",
    "mindsdb:remediation:quick": "node --experimental-strip-types scripts/generate_mindsdb_remediation_list.ts --mode quick --top 5",
    "mindsdb:remediation:full": "node --experimental-strip-types scripts/generate_mindsdb_remediation_list.ts --mode full --top 5"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@types/d3": "^7.4.3",
    "chart.js": "^4.5.1",
    "chartjs-plugin-annotation": "^3.1.0",
    "d3": "^7.9.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.3",
    "recharts": "^3.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "jsdom": "^27.4.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.17"
  }
}
